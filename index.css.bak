.workBtn{
    width:170px;
    height:60px;
    line-height: 60px;
    text-align:center;
    font-size: 50px;
    color:#fff;
    position:absolute;
    left:40%;
    top:80%;
    z-index: 111;
    -webkit-animation:rubberBand 1s ease-out infinite;
}
@-webkit-keyframes tada {
    from {
        -webkit-transform: scale3d(1, 1, 1);
        transform: scale3d(1, 1, 1);
    }

    10%, 20% {
        -webkit-transform: scale3d(.9, .9, .9) rotate3d(0, 0, 1, -3deg);
        transform: scale3d(.9, .9, .9) rotate3d(0, 0, 1, -3deg);
    }

    30%, 50%, 70%, 90% {
        -webkit-transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, 3deg);
        transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, 3deg);
    }

    40%, 60%, 80% {
        -webkit-transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, -3deg);
        transform: scale3d(1.1, 1.1, 1.1) rotate3d(0, 0, 1, -3deg);
    }

    to {
        -webkit-transform: scale3d(1, 1, 1);
        transform: scale3d(1, 1, 1);
    }
}
/*第三个页面  end*/
/*第四个页面  start*/
.cube{
    width: 100%;
    height: 100%;
    background: url(images/a.png) no-repeat;
    position: absolute;
    left: 0;
    top:0;
    perspective: 800px;
    cursor: pointer;
    display:none;
    -webkit-animation:pulse 1s linear 1;

}
@-webkit-keyframes pulse {
    from {
        -webkit-transform: scale3d(1, 1, 1);
        transform: scale3d(1, 1, 1);
    }

    50% {
        -webkit-transform: scale3d(1.05, 1.05, 1.05);
        transform: scale3d(1.05, 1.05, 1.05);
    }

    to {
        -webkit-transform: scale3d(1, 1, 1);
        transform: scale3d(1, 1, 1);
    }
}
.cubeTitle{
    width:200px;
    height:50px;
    position:absolute;
    top:5%;
    left:-100px;
    font-size:35px;
    font-weight: bold;
    color:#fff;
    text-align:center;
}
#cubeTitle{
    left:30%;
    -webkit-animation:swing 4s ease-out;
    -webkit-animation-fill-mode: both;
}
@-webkit-keyframes swing {
    20% {
        -webkit-transform: rotate3d(0, 0, 1, 15deg);
        transform: rotate3d(0, 0, 1, 15deg);
    }

    40% {
        -webkit-transform: rotate3d(0, 0, 1, -10deg);
        transform: rotate3d(0, 0, 1, -10deg);
    }

    60% {
        -webkit-transform: rotate3d(0, 0, 1, 5deg);
        transform: rotate3d(0, 0, 1, 5deg);
    }

    80% {
        -webkit-transform: rotate3d(0, 0, 1, -5deg);
        transform: rotate3d(0, 0, 1, -5deg);
    }

    to {
        -webkit-transform: rotate3d(0, 0, 1, 0deg);
        transform: rotate3d(0, 0, 1, 0deg);
    }
}
.cubeBox{
    width:389px;
    height:389px;
    position: absolute;
    left:50%;
    top:50%;
    margin-top: -255px;
    margin-left: -255px;
    transform-style: preserve-3d;
    -webkit-transform: scale(0.2) rotateX(-135deg) rotateY(-180deg);
    -webkit-transform-origin: 255px 255px -255px;
    opacity: 1;
    -webkit-transition:-webkit-transform 1s;
}
.cubeBox li{
    width: 100%;
    height: 100%;
    left:0;
    top:0;
    position: absolute;
}
.cubeBox li:nth-of-type(1){
    background: url(images/1.png);
}
.cubeBox li:nth-of-type(2){
    background: url(images/2.png);
    left:389px;
    -webkit-transform-origin: left;
    -webkit-transform: rotateY(90deg);
}
.cubeBox li:nth-of-type(3){
    background: url(images/3.png);
    left:-389px;
    -webkit-transform-origin: right;
    -webkit-transform: rotateY(-90deg);
}
.cubeBox li:nth-of-type(4){
    background: url(images/4.png);
    top:389px;
    -webkit-transform-origin: top;
    -webkit-transform: rotateX(-90deg);
}
.cubeBox li:nth-of-type(5){
    background: url(images/5.png);
    top:-389px;
    -webkit-transform-origin:bottom;
    -webkit-transform: rotateX(90deg);

}
.cubeBox li:nth-of-type(6){
    background: url(images/6.png);
    -webkit-transform: translateZ(-389px);
}
.cubeBtn{
    width:170px;
    height:60px;
    line-height:60px;
    text-align:center;
    font-size: 50px;
    color:#fff;
    position:absolute;
    left:30%;
    top:80%;
    z-index: 111;
    -webkit-animation:rubberBand 1s ease-out infinite;
}
/*第四个页面  end*/
.lastPage{
    width: 100%;
    height: 100%;
    background: url(images/e.png) no-repeat;
    position: absolute;
    z-index: 5;
    left: 0;
    top:0;
    perspective: 800px;
    cursor: pointer;
    display:none;
    -webkit-animation:pulse 10s linear infinite;
}
.lastImg{
    width:180px;
    height:270px;
    position:absolute;
    left:30%;
    top:10%;
    background: url(images/f1.gif) no-repeat;
}
.lastCon{
    width:400px;
    height:400px;
    position:absolute;
    left:30%;
    top:40%;
}
.lastCon h2{
    font-size:40px;
    color:white;
    top:-800px;
}
.lastCon p{
    position:absolute;
    top:30%;
    font-size:40px;
    line-height:50px;
    color:white;
}
.lastCon p:nth-child(even){
    -webkit-transform:translate(500px,0);

}
.lastCon p:nth-child(odd){
    -webkit-transform:translate(-500px,0);

}
#lastCon p{
    -webkit-transform:translate(0,0) rotate(720deg);
    -webkit-transition:4s ease-in-out;
}









