*{
    margin:0;
    padding:0;
}
html,body{
    width:100%;
    height:100%;
    overflow:hidden;
    font-family:"Microsoft Yahei";
}
ul,li{
    list-style:none;
}
#main{
    width:640px;
    height:960px;
    position:absolute;
    left:50%;
    top:0;
    margin-left:-310px;
    -webkit-transform-origin: top;
}
/*第一个页面  start*/
.login {
    width:100%;
    height:100%;
    position:absolute;
    left:0;
    top:0;
    background: url(images/a.png) no-repeat;
}
.logo{
    width:360px;
    height:360px;
    position:absolute;
    top:10%;
    left:20%;
    background: url(images/a1.png) no-repeat;
    -webkit-animation:rotateLogo 15s linear infinite;
}
@-webkit-keyframes rotateLogo {
    0%{
        -webkit-transform: rotate(0deg);
    }
    100%{
        -webkit-transform:rotate(360deg);
    }
}
.loginIcon{
    width:200px;
    height:111px;
    position:absolute;
    left:35%;
    bottom:20%;
    background:url(images/a2.png) no-repeat;
    -webkit-animation:bounce 1s ease-out infinite;
}
.loginClick{
    -webkit-animation:none;
}

@-webkit-keyframes bounce {
    from, 20%, 53%, 80%, to {
        -webkit-animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
        animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
        -webkit-transform: translate3d(0,0,0);
        transform: translate3d(0,0,0);
    }

    40%, 43% {
        -webkit-animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
        animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
        -webkit-transform: translate3d(0, -30px, 0);
        transform: translate3d(0, -30px, 0);
    }

    70% {
        -webkit-animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
        animation-timing-function: cubic-bezier(0.755, 0.050, 0.855, 0.060);
        -webkit-transform: translate3d(0, -15px, 0);
        transform: translate3d(0, -15px, 0);
    }

    90% {
        -webkit-transform: translate3d(0,-4px,0);
        transform: translate3d(0,-4px,0);
    }
}

.login p{
    position:absolute;
    left:45%;
    bottom:15%;
    color:lightgoldenrodyellow;
    font-size:35px;
    font-weight: bold;
    -webkit-animation:flip 1s ease-out 2;

}
@-webkit-keyframes flip {
    from {
        -webkit-transform: perspective(400px) rotate3d(0, 1, 0, -360deg);
        transform: perspective(400px) rotate3d(0, 1, 0, -360deg);
        -webkit-animation-timing-function: ease-out;
        animation-timing-function: ease-out;
    }

    40% {
        -webkit-transform: perspective(400px) translate3d(0, 0, 150px) rotate3d(0, 1, 0, -190deg);
        transform: perspective(400px) translate3d(0, 0, 150px) rotate3d(0, 1, 0, -190deg);
        -webkit-animation-timing-function: ease-out;
        animation-timing-function: ease-out;
    }

    50% {
        -webkit-transform: perspective(400px) translate3d(0, 0, 150px) rotate3d(0, 1, 0, -170deg);
        transform: perspective(400px) translate3d(0, 0, 150px) rotate3d(0, 1, 0, -170deg);
        -webkit-animation-timing-function: ease-in;
        animation-timing-function: ease-in;
    }

    80% {
        -webkit-transform: perspective(400px) scale3d(.95, .95, .95);
        transform: perspective(400px) scale3d(.95, .95, .95);
        -webkit-animation-timing-function: ease-in;
        animation-timing-function: ease-in;
    }

    to {
        -webkit-transform: perspective(400px);
        transform: perspective(400px);
        -webkit-animation-timing-function: ease-in;
        animation-timing-function: ease-in;
    }
}

/*第一个页面  end*/
/*第二个页面  start*/
.personInfo{
    width:100%;
    height:100%;
    position:absolute;
    left:0;
    top:0;
    background: url(images/b.png) no-repeat;
    display:none;
    -webkit-animation:zoomOut 1s ease-in-out;
}
@-webkit-keyframes zoomOut {
    0%{
        -webkit-transform: scale(2);
    }
    100%{
        -webkit-transform:scale(1);
    }
}
.infoList{
    width:434px;
    height:760px;
    position:absolute;
    left:15%;
    top:5%;
    border:2px solid white;
    -webkit-animation:fadeInDown 3s ease-in-out;
}

.info1{
    width:412px;
    height:277px;
    position:absolute;
    top:5px;
    left:6px;
    background: url(images/b1.png) no-repeat;
    padding:2px;
    -webkit-animation:flash 5s ease-in-out 3;
}
@-webkit-keyframes flash {
    from, 50%, to {
        opacity: 1;
    }

    25%, 75% {
        opacity: 0;
    }
}
.info1 .a1{
    position:absolute;
    left:20px;
    top:10px;
    width:70px;
    height:70px;
    background: url(images/b3.png) no-repeat;
    -webkit-animation:slideInDown 4s ease-in-out infinite;
}
.info1 .a2{
    position:absolute;
    left:90px;
    top:120px;
    width:70px;
    height:70px;
    background: url(images/b3.png) no-repeat;
    -webkit-animation:bounceInDown 27s ease-in-out infinite;
}
.info1 .a3{
    position:absolute;
    left:150px;
    top:30px;
    width:70px;
    height:70px;
    background: url(images/b3.png) no-repeat;
    -webkit-animation:fadeInDown 14s ease-in-out infinite;
}
.info1 .a4{
    position:absolute;
    left:230px;
    top:80px;
    width:70px;
    height:70px;
    background: url(images/b3.png) no-repeat;
    -webkit-animation:bounceInDown 20s ease-in-out infinite;
}
.info1 .a5{
    position:absolute;
    left:290px;
    top:20px;
    width:70px;
    height:70px;
    background: url(images/b3.png) no-repeat;
    -webkit-animation:fadeInDown 21s ease-in-out infinite;
}
@-webkit-keyframes fadeInDown {
    from {
        opacity: 0;
        -webkit-transform: translate3d(0, -100%, 0);
        transform: translate3d(0, -100%, 0);
    }

    to {
        opacity: 1;
        -webkit-transform: none;
        transform: none;
    }
}
@-webkit-keyframes slideInDown {
    from {
        -webkit-transform: translate3d(0, -100%, 0);
        transform: translate3d(0, -100%, 0);
        visibility: visible;
    }

    to {
        -webkit-transform: translate3d(0, 0, 0);
        transform: translate3d(0, 0, 0);
    }
}
@-webkit-keyframes bounceInDown {
    from, 60%, 75%, 90%, to {
        -webkit-animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
        animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
    }

    0% {
        opacity: 0;
        -webkit-transform: translate3d(0, -3000px, 0);
        transform: translate3d(0, -3000px, 0);
    }

    60% {
        opacity: 1;
        -webkit-transform: translate3d(0, 25px, 0);
        transform: translate3d(0, 25px, 0);
    }

    75% {
        -webkit-transform: translate3d(0, -10px, 0);
        transform: translate3d(0, -10px, 0);
    }

    90% {
        -webkit-transform: translate3d(0, 5px, 0);
        transform: translate3d(0, 5px, 0);
    }

    to {
        -webkit-transform: none;
        transform: none;
    }
}
.info2{
    position:absolute;
    top:280px;
    left:6px;
    color:white;
}
.info2 span{
    font-size:50px;
    -webkit-animation:bounceInDown 4s ease-in-out 1;
}
.info2 p{
    line-height:25px;
    font-size:25px;
    -webkit-animation:rotateIn 5s ease-in-out 1;

}
@-webkit-keyframes rotateIn {
    from {
        -webkit-transform-origin: center;
        transform-origin: center;
        -webkit-transform: rotate3d(0, 0, 1, -200deg);
        transform: rotate3d(0, 0, 1, -200deg);
        opacity: 0;
    }

    to {
        -webkit-transform-origin: center;
        transform-origin: center;
        -webkit-transform: none;
        transform: none;
        opacity: 1;
    }
}
@-webkit-keyframes fadeIn {
    0% {
        opacity: 0.1; /*初始状态 透明度为0*/
      }
    25% {
        opacity: 0.3; /*初始状态 透明度为0*/
    }
    50% {
        opacity: 0.5; /*中间状态 透明度为0.4*/
    }
    75% {
        opacity: 0.7; /*中间状态 透明度为0.8*/
    }
    100% {
        opacity: 0.8; /*结尾状态 透明度为1*/
    }
}
.nextBtn{
    width: 150px;
    height: 30px;
    position:absolute;
    left:35%;
    top:93%;
    color:white;
    font-size:30px;
    text-align:center;
    -webkit-animation:rubberBand 1s ease-out infinite;

}

@keyframes rubberBand {
    from {
        -webkit-transform: scale3d(1, 1, 1);
        transform: scale3d(1, 1, 1);
    }

    30% {
        -webkit-transform: scale3d(1.25, 0.75, 1);
        transform: scale3d(1.25, 0.75, 1);
    }

    40% {
        -webkit-transform: scale3d(0.75, 1.25, 1);
        transform: scale3d(0.75, 1.25, 1);
    }

    50% {
        -webkit-transform: scale3d(1.15, 0.85, 1);
        transform: scale3d(1.15, 0.85, 1);
    }

    65% {
        -webkit-transform: scale3d(.95, 1.05, 1);
        transform: scale3d(.95, 1.05, 1);
    }

    75% {
        -webkit-transform: scale3d(1.05, .95, 1);
        transform: scale3d(1.05, .95, 1);
    }

    to {
        -webkit-transform: scale3d(1, 1, 1);
        transform: scale3d(1, 1, 1);
    }
}
/*第二个页面  end*/
.skillInfo{
    width:100%;
    height:100%;
    background: url(images/g.png) no-repeat;
    position:absolute;
    top:0;
    left:0;
    display:none;
}
.skillInfo h2{
    text-align:center;
    position:absolute;
    top:3%;
    left:35%;
    font-size:45px;
    color:white;
    -webkit-animation:rubberBand 1s ease-out infinite;
}
.skillInfo hr{
    position:absolute;
    top:10%;
    left:20%;
    color:white;
    -webkit-animation:tada 1s ease-out;
}
.skillInfo div{
    position:absolute;
    background:rgba(255,255,255,0.6);
    height:14%;
    left:5%;
}
.skillInfo div p{
    font-size:28px;
    color:black;
}
.skillInfo div:nth-child(3){
    top:13%;
    width:85%;

}
.skillInfo div:nth-child(4){
    top:33%;
    width:75%;
    animation-delay: 0.3s;
}
.skillInfo div:nth-child(5){
    top:53%;
    width:65%;
    animation-delay: 0.4s;
}
.skillInfo div:nth-child(6){
    top:73%;
    width:55%;
    animation-delay: 0.5s;
}
.skillInfo div.move {
    -webkit-animation-name: myMove;
    animation-name: myMove;

    -webkit-animation-duration: 2s;
    animation-duration: 2s;

    -webkit-animation-timing-function: linear;
    animation-timing-function: linear;
}
@-webkit-keyframes myMove{
    25% {
        left: -100%;
    }
    50%, 100% {
        left: 5%;
    }
    75% {
        left: 80%;
    }
}
.skillInfo span{
    width:170px;
    height:60px;
    line-height: 60px;
    text-align:center;
    font-size:30px;
    color:#fff;
    position:absolute;
    left:40%;
    top:87%;
    -webkit-animation:rubberBand 1s ease-out infinite;
}
/*第三个页面  start*/
.workExp{
    width:100%;
